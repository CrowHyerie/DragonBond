<Defs>

  <!-- Faction Names -->
  <RulePackDef>
    <defName>VeloniriFactionNames</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=0.35)->[VeloniriPlaceNameUtility]</li>
        <li>r_name->the [political_union_medieval] of [VeloniriPlaceNameUtility]</li>
        <li>r_name->[VeloniriPlaceNameUtility] [political_union_medieval]</li>
      </rulesStrings>
      <rulesFiles>
        <li>political_union_medieval->Words/Nouns/Veloniri_Kingdom</li>
        <li>VeloniriPlaceNameUtility->Words/Nouns/Veloniri_Place_Names</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>VeloniriSettlementNames</defName>
    <rulePack>
      <include>
        <li>NamerAnimalDragonMale</li>
        <li>NamerAnimalDragonFemale</li>
      </include>
      <rulesStrings>
        <li>r_name(p=4)->[VeloniriPlaceNameUtility] [medievalSettlementEnd]</li>
        <li>r_name->[NamerAnimalDragonMale]'s [medievalSettlementEnd]-on-the-[TerrainFeature]</li>
        <li>r_name->[NamerAnimalDragonFemale]'s [medievalSettlementEnd]-by-the-[TerrainFeature]</li>
      </rulesStrings>
      <rulesFiles>
        <li>medievalSettlementEnd->Words/Nouns/PlaceEnd_Veloniri</li>
        <li>VeloniriPlaceNameUtility->Words/Nouns/Veloniri_Place_Names</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

  <!-- Pawn Names -->
  <RulePackDef>
    <defName>NamerVeloniriUtility</defName>
    <rulePack>
      <rulesFiles>
        <li>Veloniri_Male_First->Names/Veloniri_Male_First</li>
        <li>Veloniri_Female_First->Names/Veloniri_Female_First</li>
		    <li>Veloniri_Name_Last->Names/Veloniri_Name_Last</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>VeloniriNamer</defName>
	<include>
      <li>NamerVeloniriUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=2)->[Veloniri_Male_First] [Veloniri_Name_Last]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>FemaleVeloniriNamer</defName>
	<include>
      <li>NamerVeloniriUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=2)->[Veloniri_Female_First] [Veloniri_Name_Last]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
