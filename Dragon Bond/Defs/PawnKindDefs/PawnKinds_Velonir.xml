<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <PawnKindDef Name="VelonirBase" Abstract="true">
    <race>Human</race>
    <minGenerationAge>18</minGenerationAge>
    <maxGenerationAge>50</maxGenerationAge>
    <defaultFactionType>VelosEnclave</defaultFactionType>
    <!-- Default faction -->
    <chemicalAddictionChance>0.02</chemicalAddictionChance>
    <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
    <humanPregnancyChance>0.1</humanPregnancyChance>
    <invNutrition>3</invNutrition>
    <invFoodDef>Pemmican</invFoodDef>
    <itemQuality>Normal</itemQuality>
    <gearHealthRange>
      <min>1.0</min>
      <max>2.0</max>
    </gearHealthRange>
    <techHediffsMoney>0~0.5</techHediffsMoney>
    <techHediffsTags></techHediffsTags>
    <disallowedTraits>
      <li>Wimp</li>
      <li>Nudist</li>
    </disallowedTraits>
    <backstoryFiltersOverride>
      <li>
        <categoriesChildhood>
          <li>Tribal</li>
        </categoriesChildhood>
        <categoriesAdulthood>
          <li>TribalHunter</li>
        </categoriesAdulthood>
      </li>
    </backstoryFiltersOverride>
    <useFactionXenotypes>false</useFactionXenotypes>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
  </PawnKindDef>

  <!-- Velonir Peasant -->
  <PawnKindDef ParentName="VelonirBase">
    <defName>VelonirPeasant</defName>
    <label>peasant</label>
    <combatPower>35</combatPower>
    <itemQuality>Poor</itemQuality>
    <isFighter>true</isFighter>
    <gearHealthRange>
      <min>1.0</min>
      <max>2.0</max>
    </gearHealthRange>
    <weaponTags>
      <li>MedievalMeleeDecent</li>
    </weaponTags>
    <apparelTags>
      <li>BasicClothing</li>
      <li>Neolithic</li>
    </apparelTags>
    <weaponMoney>
      <min>150</min>
      <max>250</max>
    </weaponMoney>
    <apparelMoney>
      <min>200</min>
      <max>400</max>
    </apparelMoney>
    <initialWillRange>2~4</initialWillRange>
    <initialResistanceRange>14~22</initialResistanceRange>
    <xenotypeSet>
      <xenotypeChances>
        <VelonirRace>0.5</VelonirRace>
        <Baseliner>0.5</Baseliner>
      </xenotypeChances>
    </xenotypeSet>
  </PawnKindDef>
    
