<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Velos Enclave -->
  <FactionDef Name="VelosEnclaveFactionBase" ParentName="FactionBase">
    <defName>VelosEnclave</defName>
    <label>Velos Enclave</label>
    <leaderTitle>monarch</leaderTitle>
    <listOrderPriority>10</listOrderPriority>
    <settlementGenerationWeight>1</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <factionNameMaker>VeloniriFactionNames</factionNameMaker>
    <factionIconPath>World/VelosEnclaveBaseIcon</factionIconPath>
    <settlementNameMaker>VeloniriSettlementNames</settlementNameMaker>
    <settlementTexturePath>World/VelosEnclaveBaseIcon</settlementTexturePath>
    <allowedCultures><li>VeloniriCulture</li></allowedCultures>
    <techLevel>Medieval</techLevel>
     <backstoryCategories>
      <li>Tribal</li>
      <li>Medieval</li>
    </backstoryCategories>
    <description>The Velos Enclave represents the noble and traditionalist Velonir who revere their ancient bond with dragons. They believe in preserving their culture, upholding the sacred rites, and using their psychic abilities to protect and nurture both their people and their dragon companions. The Enclave seeks peaceful coexistence with other races, offering their unique skills and knowledge in exchange for mutual respect and cooperation.</description>
    <colorSpectrum>
      <li>(153, 204, 255)</li>
      <li>(102, 178, 255)</li>
    </colorSpectrum>
    <disallowedMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">Transhumanist</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">HumanPrimacy</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">MaleSupremacy</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">FemaleSupremacy</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Nudism</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
    </disallowedMemes>
    <caravanTraderKinds>
      <li>Caravan_Neolithic_BulkGoods</li>
      <li>Caravan_Neolithic_WarMerchant</li>
      <li>TraderKindDef_VelosEnclave</li>
    </caravanTraderKinds>
    <visitorTraderKinds>
      <li>Visitor_Neolithic_Standard</li>
      <li>TraderKindDef_VelosEnclave</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>Base_Neolithic_Standard</li>
      <li>TraderKindDef_VelosEnclave</li>
    </baseTraderKinds>
    <xenotypeSet Inherit="False">
      <xenotypeChances>
        <VelonirRace>0.8</VelonirRace>
        <Baseliner>0.2</Baseliner>
      </xenotypeChances>
    </xenotypeSet>
    <maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
    <configurationListOrderPriority>25</configurationListOrderPriority>
    <!-- Raid Loot Value -->
    <raidLootValueFromPointsCurve>
      <points>
        <li>(0, 15)</li>
        <li>(100, 120)</li>
        <li>(1000, 500)</li>
        <li>(2000, 800)</li>
        <li>(4000, 1000)</li>
      </points>
    </raidLootValueFromPointsCurve>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      <li>
        <!-- Normal fights, ranged with melee mix-ins -->
        <kindDef>Combat</kindDef>
        <commonality>100</commonality>
        <options>
          <VelonirPeasant>5</VelonirPeasant>
          <VelonirSquire>5</VelonirSquire>
          <VelonirArcher>10</VelonirArcher>
          <VelonirFootsoldier>10</VelonirFootsoldier>
          <VelonirDragonKnight>5</VelonirDragonKnight>
          <VelonirDragonRider>3</VelonirDragonRider>
          <DragonMonarch>1.5</DragonMonarch>
        </options>
      </li>
      <li>
        <!-- Normal fights, ranged only -->
        <kindDef>Combat</kindDef>
        <commonality>60</commonality>
        <options>
          <VelonirArcher>10</VelonirArcher>
          <VelonirFootsoldier>10</VelonirFootsoldier>
          <VelonirDragonRider>5</VelonirDragonRider>
        </options>
      </li>
      <li>
        <!-- Normal fights, melee only -->
        <kindDef>Combat</kindDef>
        <commonality>60</commonality>
        <options>
          <VelonirPeasant>10</VelonirPeasant>
          <VelonirSquire>10</VelonirSquire>
          <VelonirDragonKnight>10</VelonirDragonKnight>
          <DragonMonarch>5</DragonMonarch>
        </options>
      </li>
      <li>
        <!-- breach-capable fights, breachers plus ranged with melee mix-ins -->
        <kindDef>Combat</kindDef>
        <commonality>5</commonality>
        <options>
          <Tribal_Breacher>5</Tribal_Breacher>
          <VelonirPeasant>5</VelonirPeasant>
          <VelonirSquire>5</VelonirSquire>
          <VelonirArcher>10</VelonirArcher>
          <VelonirFootsoldier>10</VelonirFootsoldier>
          <VelonirDragonKnight>5</VelonirDragonKnight>
          <VelonirDragonRider>3</VelonirDragonRider>
          <DragonMonarch>1.5</DragonMonarch>
        </options>
      </li>
      <li>
        <!-- Peaceful -->
        <kindDef>Peaceful</kindDef>
        <options>
          <VelonirSquire>20</VelonirSquire>
          <Tribal_Child MayRequire="Ludeon.RimWorld.Biotech">10</Tribal_Child>
          <VelonirArcher>10</VelonirArcher>
          <VelonirFootsoldier>10</VelonirFootsoldier>
          <VelonirDragonKnight>5</VelonirDragonKnight>
          <VelonirDragonRider>3</VelonirDragonRider>
          <DragonMonarch>1.5</DragonMonarch>
        </options>
      </li>
      <!-- Trader -->
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <VelonirMerchant>1</VelonirMerchant>
        </traders>
        <carriers>
          <Muffalo>6</Muffalo>
           <Alpaca>2.5</Alpaca>
        </carriers>
        <guards>
          <VelonirSquire>7</VelonirSquire>
          <VelonirArcher>10</VelonirArcher>
          <VelonirFootsoldier>10</VelonirFootsoldier>
          <VelonirDragonKnight>7</VelonirDragonKnight>
        </guards>
      </li>
      <!-- Settlement -->
      <li>
        <kindDef>Settlement</kindDef>
        <options>
          <VelonirSquire>7</VelonirSquire>
          <VelonirArcher>10</VelonirArcher>
          <VelonirFootsoldier>10</VelonirFootsoldier>
          <VelonirDragonKnight>7</VelonirDragonKnight>
          <VelonirDragonRider>10</VelonirDragonRider>
        </options>
      </li>
      <li>
        <kindDef>Settlement_RangedOnly</kindDef>
        <options>
          <VelonirArcher>10</VelonirArcher>
          <VelonirFootsoldier>10</VelonirFootsoldier>
          <VelonirDragonRider>10</VelonirDragonRider>
        </options>
      </li>
    </pawnGroupMakers>
  </FactionDef>

