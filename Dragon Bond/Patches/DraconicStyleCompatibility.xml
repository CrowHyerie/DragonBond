<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>ATH's style Draconic</li>
    </mods>

    <match Class="PatchOperationSequence">
      <operations>

        <li Class="PatchOperationReplace">
        <xpath>Defs/CultureDef[defName="VeloniriCulture"]/thingStyleCategories/li[category="Rustic"]</xpath>
          <value>
            <li>
              <category>Draconic</category>
              <priority>2</priority>
            </li>
          </value>
        </li>

      </operations>
    </match>

  </Operation>
</Patch>