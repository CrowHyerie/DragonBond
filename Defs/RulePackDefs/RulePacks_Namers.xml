<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Pawn Names -->
  <RulePackDef>
    <defName>Crows_NamerVeloniriUtility</defName>
    <rulePack>
      <rulesFiles>
        <li>Veloniri_Male_First->Names/Veloniri_Male_First</li>
        <li>Veloniri_Female_First->Names/Veloniri_Female_First</li>
		    <li>Veloniri_Name_Last->Names/Veloniri_Name_Last</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <!-- Male Pawn Names -->
  <RulePackDef>
    <defName>Crows_VeloniriNamer</defName>
	<include>
      <li>Crows_NamerVeloniriUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=2)->[Veloniri_Male_First] [Veloniri_Name_Last]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <!-- Female Pawn Names -->
  <RulePackDef>
    <defName>Crows_FemaleVeloniriNamer</defName>
	<include>
      <li>Crows_NamerVeloniriUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=2)->[Veloniri_Female_First] [Veloniri_Name_Last]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  

</Defs>