<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <!-- Patch to add the custom HediffGiver_DragonBond to the OrganicDragon hediffGiverSet -->
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/HediffGiverSetDef[defName="OrganicDragon"]/hediffGivers</xpath>
    <value>
      <li Class="DragonBond.HediffGiver_DragonBond">
        <hediff>Crows_DragonBondHediff</hediff>
      </li>
    </value>
  </Operation>
</Patch>
